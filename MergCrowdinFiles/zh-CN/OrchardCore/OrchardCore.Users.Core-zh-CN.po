#. reportError("Password", T["A password is required."]);
#: OrchardCore.Users.Core\Services\UserService.cs:100
msgctxt "OrchardCore.Users.Services.UserService"
msgid "A password is required."
msgstr "需要密码"

#. reportError("Token", T["A token is required."]);
#: OrchardCore.Users.Core\Services\UserService.cs:106
msgctxt "OrchardCore.Users.Services.UserService"
msgid "A token is required."
msgstr "需要token"

#. reportError("Password", T["Passwords must have at least one digit ('0'-'9')."]);
#: OrchardCore.Users.Core\Services\UserService.cs:165
msgctxt "OrchardCore.Users.Services.UserService"
msgid "Passwords must have at least one digit ('0'-'9')."
msgstr "密码必须至少包含一个数字（'0'-'9'）。"

#. reportError("Password", T["Passwords must have at least one lowercase ('a'-'z')."]);
